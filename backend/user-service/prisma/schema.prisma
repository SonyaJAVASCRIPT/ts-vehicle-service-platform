generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("USER_DATABASE_URL")
}

model User {
  id       Int    @id @default(autoincrement())
  username String
  password String
  email    String @unique

  @@map("user")
}
