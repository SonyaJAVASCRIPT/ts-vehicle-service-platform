generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://sonya:133755@localhost:5432/mydb?schema=user-service"
}

model User {
  id       Int    @id @default(autoincrement())
  username String
  password String
  email    String @unique

  @@map("user")
}
